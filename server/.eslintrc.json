{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
      "import/resolver": {
        "babel-module": {
          "alias": {
            "map": [
              ["@", "./src"]
            ],
            "extensions": [".ts", ".js", ".jsx", ".json"]
          }
        }
      }
    }
}
